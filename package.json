{
  "name": "financials",
  "version": "1.0.0",
  "description": "Financial algorithms in TypeScript",
  "main": "lib/index",
  "types": "lib/index",
  "scripts": {
    "build:live": "nodemon -V -w ./config -w ./src -w ./web --exec ts-node ./lib/index.js",
    "clean": "rm -fR ./lib ./coverage",
    "start": "npm run build:live",
    "start-server": "node lib/index.js",
    "test": "tslint -c tslint.json 'src/**/*.ts' && tsc && istanbul cover --report json jasmine lib/*.spec.js lib/**/*.spec.js && remap-istanbul -i coverage/coverage.json -o coverage/html-report -t html",
    "test-no-coverage": "tslint -c tslint.json 'src/**/*.ts' && tsc && jasmine  lib/*.spec.js lib/**/*.spec.js"
  },
  "author": "jwright",
  "license": "BSD-Source-Code",
  "devDependencies": {
    "@types/jasmine": "^2.5.47",
    "@types/lodash": "^4.14.64",
    "@types/node": "^7.0.31",
    "istanbul": "^0.4.5",
    "jasmine": "^2.6.0",
    "jasmine-spec-reporter": "^4.2.1",
    "nodemon": "^1.11.0",
    "remap-istanbul": "^0.9.5",
    "through": "^2.3.8",
    "ts-node": "^3.0.6",
    "tslint": "^5.5.0",
    "typescript": "^2.3.2",
    "vinyl-source-stream": "^1.1.0"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.1",
    "cookie-parse": "^0.4.0",
    "csv-parse": "^1.2.0",
    "glue": "^4.1.0",
    "hapi": "^16.4.3",
    "inert": "^4.2.0",
    "knex": "^0.13.0",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "pg": "^6.2.4",
    "stream-transform": "^0.1.2",
    "through": "^2.3.8"
  }
}
